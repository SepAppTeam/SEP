<div id="example" class="page-header">
    <h3>Estudiantes</h3>
    <div class="navbar">
    <div class="navbar-inner">
    <li id='buscar' class="brand" href="#">Buscar</li>
      <div class="row">
        <div class="span6">
          <form method="GET" class="form-inline" >
            <input id='buscar' name="buscar" class="search-query" type="text" placeholder="Buscar" >
            <button id='buscar' type="submit" class="btn btn-primary"><i class="icon-search icon-white"></i></button>
          </form>
        </div>
      </div>
    </div>
  </div>
  </div>
  <table class="table table-hover"  >
    <thead>
      <tr>
        <th >id</th>
        <th >Nombre1</th>
        <th >Apellido1</th>  
        <th >Apellido2</th> 
        <th >Genero</th>
        <th >TipoDocumento</th>
        <th >#Documento</th> 
        <th >TipoPracticas</th>
        <th >Fichas</th>
        <th >Jefes</th>
        <th >Acciones</th>       
      </tr>
    </thead>
    <tbody><%= render @estudiantes %></tbody>
    <tfoot>
      <tr>
        <td>
          <%= link_to image_tag('Crud/save.png'),  new_estudiante_path,  :remote => true %>
        </td>
      </tr>
    </tfoot>
  </table>
  <div id="pagination">
    <ul class="pagination">
      <%= will_paginate @estudiantes, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente&raquo;' %>
    </ul>
  </div>
      