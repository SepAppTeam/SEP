<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>Jefes</h3>
</div>
  <div class="modal-body">
    <table class="table table-striped table-condensed">
      <tr>
        <td>ID: </td>
        <td><%= @jefe.id %></td>
      </tr>
      <tr>
        <td><b >Nombre: </b></td>
        <td><%= @jefe.nombres %></td>
      </tr>
      <tr>
        <td><b >Apellidos: </b></td>
        <td><%= @jefe.apellidos %></td>
      </tr>
        <tr>
        <td><b >Cargos: </b></td>
        <td><%= @jefe.cargo %></td>
      </tr>
      <tr>
        <td><b >Telefono: </b></td>
        <td><%= @jefe.telefono %></td>
      </tr>
      <tr>
        <td><b >Correo: </b></td>
        <td><%= @jefe.email %></td>
      </tr>
       <tr>
        <td><b >Empresas: </b></td>
        <td><%= @jefe.empresa.nombre %></td>
      </tr>
    </table>
    <div class="modal-footer">
      <a onclick="$.lazybox.close()">&times;</a>
    </div>           
</div>


