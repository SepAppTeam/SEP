<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>detalle evaluaciones</h3>
</div>
  <div class="modal-body">
    <table class="table table-striped table-condensed">
      <tr>
        <td><b class="pull-right">ID: </b></td>
        <td><%= @detalle_evaluacion.id %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Evaluaciones: </b></td>
        <td><%= @detalle_evaluacion.evaluacion_id %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Factores: </b></td>
        <td><%= @detalle_evaluacion.factor_id %></td>
      </tr>
       <tr>
        <td><b class="pull-right">Observaciones: </b></td>
        <td><%= @detalle_evaluacion.observaciones %></td>
      </tr>
    </table>
    <div class="modal-footer">
      <a onclick="$.lazybox.close()">&times;</a>
    </div>           
</div>


