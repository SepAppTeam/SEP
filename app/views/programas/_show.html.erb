<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>Programas</h3>
</div>
  <div class="modal-body">
    <table class="table table-striped table-condensed">
      <tr>
        <td><b class="pull-right">ID: </b></td>
        <td><%= @programa.id %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Nombre: </b></td>
        <td><%= @programa.nombre %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Versiones: </b></td>
        <td><%= @programa.version %></td>
      </tr>

       <tr>
        <td><b class="pull-right">Codigos: </b></td>
        <td><%= @programa.codigo %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Centros: </b></td>
        <td><%= @programa.centro.nombre %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Titulaciones: </b></td>
        <td><%= @programa.titulacion.descripcion %></td>
      </tr>
    </table>
    <div class="modal-footer">
      <a onclick="$.lazybox.close()">&times;</a>
    </div>           
</div>


