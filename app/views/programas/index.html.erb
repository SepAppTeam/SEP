<h3>Datos Programas</h3>
  <table class="table table-hover"  >
    <thead>
      <tr>
        <th >id</th>
        <th >Nombres</th>
        <th >Versiones</th>
        <th >Codigos</th>
        <th >Centros</th>
        <th >Titulaciones</th>    
        <th >Acciones</th>      
      </tr>
    </thead>
    <tbody>
      <%= render @programas %>
    </tbody>
    <tfoot>
      <tr>
        <td>
          <%= link_to image_tag('Crud/save.png'),  new_programa_path,  :remote => true %>
        </td>
      </tr>
    </tfoot>
  </table>
      