<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>TiposFactores</h3>
</div>
  <div class="modal-body">
    <table class="table table-striped table-condensed">
      <tr>
        <td><b class="">ID: </b></td>
        <td><%= @tipo_factor.id %></td>
      </tr>
      <tr>
        <td><b class="">Nombre: </b></td>
        <td><%= @tipo_factor.nombre %></td>
      </tr>
      <tr>
        <td><b class="">Siglas: </b></td>
        <td><%= @tipo_factor.sigla %></td>
      </tr>
    </table>
    <div class="modal-footer">
      <a onclick="$.lazybox.close()">&times;</a>
    </div>           
</div>


