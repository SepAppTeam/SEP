<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>TipoDocumentos</h3>
</div>
  <div class="modal-body">
    <table class="table table-striped table-condensed">
      <tr>
        <td><b class="pull-right">ID: </b></td>
        <td><%= @tipodoc.id %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Sigla: </b></td>
        <td><%= @tipodoc.sigla %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Descripciones: </b></td>
        <td><%= @tipodoc.descripcion %></td>
      </tr>
    </table>
    <div class="modal-footer">
      <a onclick="$.lazybox.close()">&times;</a>
    </div>           
</div>


