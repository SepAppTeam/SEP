<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>Areas</h3>
</div>
<div class="modal-body">
  <table class="table table-striped table-condensed">
    <tr>
      <td><b class="pull-right">ID: </b></td>
      <td><%= @area.id %></td>
    </tr>
    <tr>
      <td><b class="pull-right">Nombre: </b></td>
      <td><%= @area.nombre %></td>
    </tr>
    <tr>
      <td><b class="pull-right">Siglas: </b></td>
      <td><%= @area.sigla %></td>
    </tr>
  </table>
  <div class="modal-footer">
    <a onclick="$.lazybox.close()">&times;</a>
  </div>           
</div>


